   "source": [
    "from prefect import task, flow\n",
    "\n",
    "@task\n",
    "def printer(obj):\n",
    "    print(f\"Received a {type(obj)} with value {obj}\")\n",
    "\n",
    "# note that we define the flow with type hints\n",
    "@flow\n",
    "def validation_flow(x: int, y: str):\n",
    "    printer(x)\n",
    "    printer(y)\n",
    "\n",
    "validation_flow(x=\"42\", y=100)\n"
   ]
